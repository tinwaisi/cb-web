{
  "env": {
    "node": 1
  },
  "rules": {
    "quotes": [
      2,
      "single"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "no-new": 0,
    "no-multi-spaces": 0,
    "new-cap": 0,
    "no-process-exit": 0,
    "no-underscore-dangle": 0,
    "no-console": 2,
    "no-use-before-define": [
      2,
      "nofunc"
    ]
  }
}